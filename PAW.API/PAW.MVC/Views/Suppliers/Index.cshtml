@model IEnumerable<PAW.Models.Supplier>
@{
    ViewData["Title"] = "Index";
}


<h1>Suppliers</h1>

<div class="container">
    @foreach (var s in Model)
    {
        <ul>
            <li>@s.SupplierId</li>

            @switch (s.SupplierName)
            {
                case "Exotic Liquids":
                    <li>@s.SupplierName</li>
                    break;
                case "New Orleans Cajun Delights":
                    <li>New Orleans Cajun Delights</li>
                    break;
            }
            <li>@s.ContactName</li>
            <li>@s.ContactTitle</li>
            <li>@s.Phone</li>
            <li>@s.Address</li>
            <li>@s.City</li>
            <li>@s.Country</li>
        </ul>
    }
</div>

